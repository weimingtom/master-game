<walls coords="-5,-4 -4,-4 -4,-1 -4,1 -4,3 -3,1 -3,2 -3,3 -2,-4 -2,-2 -2,-1 -2,1 -2,2 -2,3 -1,-2 -1,0 -1,2 -1,3 0,-4 0,0 1,0 2,-2 2,-1 3,-3 3,-1 3,1 3,3 4,-3 "/>

<!-- спрайты - указываются координаты источника на текстуре и номер -->

<sprite id="1" left="130" top="150" right="170" bottom="190"/>
<sprite id="2" left="390" top="160" right="430" bottom="200"/>


<!-- шаблоны компонентов - указываются значения параметров во умлочанию -->
<goc name="CompPhys">
	<pos value="5,5"/>
</goc>

<goc name="CompVisualSq">
	<sprite value="1"/>
</goc>


<!-- шаблоны объектов - указывается, какие компоненты входят в объект. Можно переназначить значения параметров по умолчанию -->

<object_template name="GuestTemplate">
	<components>
		<goc name="CompVisualSq">
			<sprite value="2"/>
		</goc>
		<goc name="CompPhys">
			<pos value="10,0"/>
		</goc>
	</components>
</object_template>
<object_template name="wallTemplate">
	<components>
		<goc name="CompVisualSq">
			<sprite value="1"/>
		</goc>
		<goc name="CompPhys">
			<pos value="0,10"/>
		</goc>
	</components>
</object_template>


<!-- создание объектов. Можно переназначить значения параметров по умолчанию. -->


<object name="guest" template="GuestTemplate">
	<components>
		<goc name="CompPhys">
			<pos value="5,5"/>
		</goc>
	</components>
</object>

<object name="wall" template="wallTemplate">
	<components/>
</object>

